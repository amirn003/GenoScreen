<div class="container">
  <div class="row">
    <div class="col">
      <h1>Analysis#show</h1>
      <h2><%= @analysis.name %></h2>
         <h2><%= @users.find(@analysis.user_id).username %></h2>

      <div class="button-container">
        <%= link_to "Back to Analysis", analysis_index_path, class: "btn-primary" %>
        <%= link_to "Edit", edit_analysis_path(@analysis), class: "btn-primary" %>
        <%= button_to 'Destroy', @analysis, class: "btn-primary", method: :delete, data: { confirm: 'Are you sure?' } %>
    </div>
    </div>
  </div>
</div>
